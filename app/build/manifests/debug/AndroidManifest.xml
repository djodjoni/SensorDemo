<manifest xmlns:ads="http://schemas.android.com/apk/lib/com.google.ads" xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="5" android:versionName="1.3" package="org.djodjo.test.sensortest">

    <uses-sdk android:minSdkVersion="9" android:targetSdkVersion="19"/>
